/* pages/index/index.wxss */

.main, page {
  width: 100%;
  height: 1000%;
  overflow: hidden;
}

.main {
  display: flex;
  flex-direction: column;
  margin: 100rpx auto;
}

.main>view.sol {
  height: 300rpx;
  border: 2rpx solid #ccc;
  margin: 30rpx 0;
}
.main>view>view>image{
  width: 196rpx;
  height: 135rpx;
}
.sol{
  display: flex;
  justify-content: space-around;
  text-align: center;
  align-items: center;
}