body,html,page,.main{
    width:100%;
    height:100%;
}
.main{
    display:flex;
    flex-direction:column;
}
.main>.texts{
    width: 100%;
    display:flex;
    height:120rpx;
    align-items:center;
    padding:0 10rpx;
    box-sizing:border-box;
}
.main>.texts>.left{
    flex:1;
    display:flex;
}
.main>.texts>.left>.imgs{
    width:100rpx;
    display:flex;
}
.main>.texts>.left>.imgs>image{
    width:60rpx;
    height:60rpx;
    margin:0 10rpx;
}
.main>.texts>.left>.conts{
    flex:1;
    padding:0 25rpx;
    box-sizing:border-box;
}
.main>.texts>.left>.conts>view{
    display:flex;
    width:100%;
    justify-content:space-between;
}
.main>.texts>.right{
    width:80rpx;
    text-align:center;
}
.main>.texts>.right>image{
    width:30rpx;
    height:30rpx;
}
