<view class='main'>
  <view class='header'>
      <swiper indicator-dots="{{indicatorDots}}" autoplay= "{{autoplay}}"  interval="{{interval}}" duration="{{duration}}">
          <block wx:for="{{imgUrls}}">
              <swiper-item>
                  <image src="{{item}}" class="slide-image" />
              </swiper-item>
          </block>
      </swiper>
  </view>
  <view class='section'>
        <view class='navTab'>
      <swiper autoplay= "{{autoplay}}"  interval="{{interval}}" duration="{{duration}}">
          <block wx:for="{{list}}">
              <swiper-item class='dls'>
                 <view wx:for='{{item.children}}' class='nav'>
                    <image src='{{item.img}}'></image>
                    <view>{{item.text}}</view>
                 </view>
              </swiper-item>
          </block>
      </swiper>
        </view>
    
  </view>
  <view class='footer'>
   
  </view>
</view>