<view class="main">
    <view class='texts' wx:for='{{list}}' wx:key='{{item.id}}'>
      <view class='left'>
        <view class='imgs'>
            <view>{{index+1}}</view>
            <image src="{{item.pic}}"></image>
        </view>
        <view class='conts'>
            <view><view>{{item.name}}</view><view>{{item.num}}</view></view>
            <progress percent='{{item.num*100/50000}}'></progress>
        </view>
      </view>
      <view class='right'>
            <view>{{!item.flag?0:1}}</view>
            <image src='{{item.iconpath}}' bindtap="handljialing" data-id='{{item.id}}'></image>
      </view>
    </view>
</view>
